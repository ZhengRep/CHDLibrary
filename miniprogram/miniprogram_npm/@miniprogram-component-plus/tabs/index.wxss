.weui-tabs{width:100%}
.weui-tabs-bar__wrp{width:100%;}
.weui-tabs-bar__content{width:100%;white-space:nowrap}
.weui-tabs-bar__item{display:inline-block}
.weui-tabs-bar__title{display:inline-block;border-bottom-width:2px;border-bottom-style:solid;border-bottom-color:transparent}

@media (prefers-color-scheme:light) {
    .custom-color{
        background-color: #ffffff;
    }
}
@media (prefers-color-scheme:dark) {
    .custom-color{
        background-color: #191919;
    }
}